{
  "expo": {
    "name": "swift-scan",
    "slug": "swift-scan",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Gelasio-Bold.ttf",
            "./assets/fonts/Gelasio-BoldItalic.ttf",
            "./assets/fonts/Gelasio-Italic.ttf",
            "./assets/fonts/Gelasio-Medium.ttf",
            "./assets/fonts/Gelasio-MediumItalic.ttf",
            "./assets/fonts/Gelasio-Regular.ttf",
            "./assets/fonts/Gelasio-SemiBold.ttf",
            "./assets/fonts/Gelasio-SemiBoldItalic.ttf"
          ]
        }
      ]
    ]
  }
}
